<VirtualHost *:<%= @port %>>
        ServerAdmin <%= @serveradmin %>
        ServerName <%= @servername %>
        ServerAlias www.<%= @servername %>
        DocumentRoot <%= @document_root %>
	ErrorLog <%= @error_log %>/error.log
	CustomLog <%= @error_log %>/access.log combined
        <Directory <%= @document_root %>>
                Require all granted
        </Directory>
</VirtualHost>
